{
  "expo": {
    "name": "LadyBug",
    "slug": "LadyBug",
    "privacy": "public",
    "description": "Safety App, Pulls real-time crime data and displays it on the app.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "com.mya.lb",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/loader.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mya.lb"
    },
    "android": {
      "package": "com.mya.lb",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
